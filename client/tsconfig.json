{
	"compilerOptions": {
		"target": "ES2019",
		"module": "ESNext",
		"jsx": "react-jsx",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"moduleResolution": "node",
		"outDir": "./dist",
		"rootDir": "./src",
		"allowSyntheticDefaultImports": true
	},
	"include": ["src/**/*"],
	"exlude": ["node_modules"]
}
